    <TargetFrameworks Condition="'$(BuildProjectFramework)' == '' and '$(OS)' == 'Windows_NT'">net462;$(SupportedSubrocessNetTargetFrameworks)</TargetFrameworks>
  </PropertyGroup>
</Project>
